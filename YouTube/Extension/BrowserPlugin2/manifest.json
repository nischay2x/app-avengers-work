{
    "name" : "YouTube Mint",
    "description" : "Add your videos to YT Mint",
    "version" : "1.0.0",
    "manifest_version" : 3,
    "key" : "",
    "icons" : {
        "19" : "./icon.png",
        "128" : "./icon_128.png"
    },
    "background" : {
        "service_worker" : "./background.js"
    },
    "action" : {
        "default_popup" : "popup.html"
    },
    "permissions" : [
        "tabs",
        "storage",
        "activeTab",
        "scripting"
    ],
    "host_permissions" : [
        "https://www.youtube.com/"
    ]
}